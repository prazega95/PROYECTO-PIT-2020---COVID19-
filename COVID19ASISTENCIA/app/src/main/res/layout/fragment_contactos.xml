<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".contactos">


    <TextView
        android:id="@+id/textView13"
        android:layout_width="238dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="-150dp"
        android:fontFamily="@font/light"
        android:textSize="30sp"
        android:layout_gravity="center"
        android:text="Linea de atencion"
         />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="238dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="-100dp"
        android:text="COVID-19"

        android:textSize="50sp"
        android:layout_gravity="center"
        android:fontFamily="@font/regular"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginTop="320dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="123dp"
            android:layout_height="33dp"
            android:layout_gravity="center"
            android:layout_marginLeft="65dp"
            android:src="@drawable/llamarsamu" />

        <Button
            android:id="@+id/btnLLAMARSamu"
            android:layout_width="120dp"
            android:layout_height="25dp"
            android:layout_gravity="center"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="60dp"
            android:textColor="@drawable/efecto_clic_button"
            android:background="@drawable/button"
            android:fontFamily="@font/regular"
            android:text="Marcar" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="360dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="131dp"
            android:layout_height="38dp"
            android:layout_gravity="center"
            android:layout_marginLeft="65dp"
            android:src="@drawable/llamaressalud" />

        <Button
            android:id="@+id/btnLLAMAREsSalud"
            android:layout_width="120dp"
            android:layout_height="25dp"
            android:layout_gravity="center"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="60dp"
            android:textColor="@drawable/efecto_clic_button"
            android:background="@drawable/button"
            android:fontFamily="@font/regular"
            android:text="Marcar" />

    </LinearLayout>


</FrameLayout>